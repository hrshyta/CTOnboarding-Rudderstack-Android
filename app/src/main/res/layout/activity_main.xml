<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="start"
    android:orientation="vertical"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/PushUserProfile"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="4dp"
        android:backgroundTint="#ff003b"
        android:onClick="onClickBtn"
        android:text="PushUserProfile" />

    <Button
        android:id="@+id/PushEvents"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:backgroundTint="#ff003b"
        android:onClick="onClickBtn3"
        android:text="PushEvents" />

    <Button
        android:id="@+id/OrderCompletedEvent"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:backgroundTint="#ff003b"
        android:onClick="onClickBtnCustomer"
        android:text="OrderCompletedEvent"
        tools:ignore="HardcodedText" />

</LinearLayout>